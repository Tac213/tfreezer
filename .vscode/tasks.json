        "command": "${workspaceFolder}\\.venv\\Scripts\\python.exe"
      },
      "args": [
        "-m",
        "tfreezer",
        "--variant",
        "debug",
        "--workpath",
        "${workspaceFolder}/build/pywin32_demo",
        "--entry-module",
        "${workspaceFolder}/examples/pywin32_demo/pywin32_demo.py"
      ],
      "group": {
        "kind": "build"
      }
    }
  ]
}
