      },
      "args": [
        "-m",
        "tfreezer",
        "--variant",
        "debug",
        "--workpath",
        "${workspaceFolder}/build/pyqt5_demo",
        "${workspaceFolder}/examples/pyqt5_demo/deploy_spec.py"
      ],
      "group": {
        "kind": "build"
      }
    },
    {
