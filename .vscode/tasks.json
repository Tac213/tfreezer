{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "basic_check",
      "type": "process",
      "command": "${workspaceFolder}/.venv/bin/python",
      "windows": {
        "command": "${workspaceFolder}\\.venv\\Scripts\\python.exe"
      },
      "args": [
        "-m",
        "tfreezer",
        "--variant",
        "debug",
        "--workpath",
        "${workspaceFolder}/build/basic_check",
        "--entry-module",
        "${workspaceFolder}/examples/basic_check/basic_check.py"
      ],
      "group": {
        "kind": "build"
      }
    },
    {
      "label": "pyqt5_demo",
      "type": "process",
      "command": "${workspaceFolder}/.venv/bin/python",
